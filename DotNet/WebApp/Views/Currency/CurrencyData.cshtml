@model WebApp.Models.Currency

<div class="form-group row">
    <label asp-for="CurrencyCode" class="col-sm-2 col-form-label"></label>
    <div class="col-sm-10">
        <input asp-for="CurrencyCode" class="form-control" placeholder="CurrencyCode">
        <span asp-validation-for="CurrencyCode"></span>
    </div>
</div>

<div class="form-group row">
    <label asp-for="CurrencyName" class="col-sm-2 col-form-label"></label>
    <div class="col-sm-10">
        <input asp-for="CurrencyName" class="form-control" placeholder="CurrencyName">
        <span asp-validation-for="CurrencyName"></span>
    </div>
</div>

<div class="form-group row">
    <label asp-for="CurrencyLatinName" class="col-sm-2 col-form-label"></label>
    <div class="col-sm-10">
        <input asp-for="CurrencyLatinName" class="form-control" placeholder="CurrencyLatinName">
        <span asp-validation-for="CurrencyLatinName"></span>
    </div>
</div>

<div class="form-group row">
    <label asp-for="OrderNum" class="col-sm-2 col-form-label"></label>
    <div class="col-sm-10">
        <input asp-for="OrderNum" class="form-control" placeholder="OrderNum">
        <span asp-validation-for="OrderNum"></span>
    </div>
</div>

@Html.HiddenFor(m => m.Date)

<div class="form-group row">
    <div class="col-sm-10">
        <button type="submit" class="btn btn-primary">Save</button>
    </div>
</div>
